{{#each this}}
    <div class="card news-card js-new-card">
    <div class='news-image'>
        <img src="{{urlToImage}}" alt="">
    </div>
    <h2 class="card-title">{{title}}</h2>
    <p class="card-desc">{{description}}</p>
    <div class="card-footer">
        <span>{{author}}</span>
        <span>{{publishedAt}}</span>
    </div>
</div>
{{/each}}